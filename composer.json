{
    "name": "yiisoft/data",
    "type": "library",
    "description": "Data providers",
    "keywords": [
        "data provider"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/data/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/data"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/arrays": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Data\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Data\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
